{
  "name": "migrated",
  "version": "1.0.0",
  "description": "A simple way to migrate objects.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "keywords": ["migrated"],
  "repository": {
    "type": "git",
    "url": "https://github.com/paulinalewand/migrated"
  },
  "author": {
    "name": "Paulina",
    "url": "https://github.com/paulinalewand"
  },
  "homepage": "https://github.com/paulinalewand#readme",
  "bugs": {
    "url": "https://github.com/paulinalewand/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  }
}
